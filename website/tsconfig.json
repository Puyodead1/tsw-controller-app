  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "isolatedModules": true,
    "types": ["vite/client"],
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ]
}